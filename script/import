#!/usr/bin/env rails runner

DAV    = false
FILE   = true
DELETE = true

if DAV
  account = ContactDavImportAccount.find 1
  account.password = ARGV[0]
  account.contacts.delete_all if DELETE
  account.import(5000)

  account = ContactDavImportAccount.find 2
  account.password = ARGV[0]
  account.contacts.delete_all if DELETE
  account.import(5000)

  account = ContactDavImportAccount.find 3
  account.password = ARGV[0]
  account.contacts.delete_all if DELETE
  account.import(5000)
end

if FILE
  account = ContactFileImportAccount.find 10
  account.contacts.delete_all if DELETE
  account.import

end
